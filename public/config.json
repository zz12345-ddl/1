{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "PROJECTNAME",
    "version": "PROJECTVERSION",
    "identifier": "PROJECTID",
    "app": {
        "security": {
            "csp": null,
            "dangerousDisableAssetCspModification": true,
            "assetProtocol": {
                "enable": true,
                "scope": {
                    "allow": ["**/*"],
                    "requireLiteralLeadingDot": false
                }
            }
        },
        "withGlobalTauri": -2
    },
    "bundle": {
        "active": true,
        "targets": -3,
        "resources": ["./data/*"],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
